  </ItemGroup>

  <ItemGroup>
    <None Update="Images\amogus.jfif">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\botinok.jfif">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\popich.jfif">
